{
  "manifest_version": 2,
  "name": "Movie quotes extension",
  "version": "2.0",
  "description": "Movie quotes extension",

  "browser_action": {
    "default_icon": "logo.png"
  },

    "chrome_url_overrides": {
      "newtab": "quotes.html"
    },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "geolocation"
  ]
}
